<!doctype html>
<html class="no-js">
    <head>
        <meta charset="utf-8" />
        <meta http-equiv="x-ua-compatible" content="ie=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0" />

        <title>__TEMPLATE__ template</title>


        <link href="/resources/templates/__TEMPLATE__/stylesheets/__TEMPLATE__.css" type="text/css" rel="stylesheet">

        {% block page_style %}{% endblock %}
        {% block page_stylesheets %}{% endblock %}
    </head>
    <body>
        <div class="sticky-end">
            <div class="sticky-end-body">
                {% block header %}{% endblock %}

                {% block content %}
                    No content defined.
                {% endblock %}
            </div>
            <div class="sticky-end-footer">
                {% block footer %}{% endblock %}
            </div>
        </div>
    </body>

    <script src="/resources/templates/__TEMPLATE__/javascripts/__TEMPLATE__.js"></script>

    {% block page_script %}{% endblock %}
    {% block page_javascripts %}{% endblock %}
</html>